        "def f(n):\n",
        "  f=1\n",
        "  if n < 0:\n",
        "    print(\"It is a negative no.\")\n",
        "  elif n==0:\n",
        "    print(\"the factorial of 0 is 1\")\n",
        "  else: \n",
        "    for i in range(1 , n + 1):\n",
        "      f= f * i\n",
        "      print(\"Factorial of\", n ,\"is :\" , f)\n",
        "\n",
        "\n",
        "n = int(input(\"Enter a number :\"))\n",
        "f(n)\n",
        "      "
